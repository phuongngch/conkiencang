<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="admin-2columns-left" xsi:noNamespaceSchemaLocation="../../../../../../../lib/internal/Magento/Framework/View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <block class="Gos\Tradein\Block\Adminhtml\Tradein\Edit" name="gos_tradein_tradein_edit"/>
        </referenceContainer>
        <referenceContainer name="left">
            <block class="Gos\Tradein\Block\Adminhtml\Tradein\Edit\Tabs" name="gos_tradein_tradein_tabs">
                <block class="Gos\Tradein\Block\Adminhtml\Tradein\Edit\Tab\Tradein" name="gos_tradein_tradein_edit_tab_tradein"/>
                <action method="addTab">
                    <argument name="name" xsi:type="string">tradein</argument>
                    <argument name="block" xsi:type="string">gos_tradein_tradein_edit_tab_tradein</argument>
                </action>
            </block>
        </referenceContainer>
    </body>
</page>
